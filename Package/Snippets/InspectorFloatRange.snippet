          <ID>MIN</ID>
          <Default>0</Default>
        </Literal>
        <Literal>
          <ID>MAX</ID>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>NAME</ID>
          <Default>NAME</Default>
        </Literal>
      </Declarations>
      
    </Snippet>
